<%= render 'layouts/navber' %>
<%= form_with model:@content_album, local: true do |f| %>
  <%= f.label :アルバム名 %>
  <%= f.text_field :album_name %>
  <%= f.collection_check_boxes :content_id, @contents, :id, :title do |b| %>
    <%= b.label { b.check_box + b.text } %>
  <% end %>
  <%= f.submit "保存" %>
<% end %>