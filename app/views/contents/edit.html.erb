<%= render 'layouts/navber' %>
<%= form_with model:@content, local:true do |f| %>
  <%= f.label :画像 %><br>
  <%= f.file_field :img, class: "image" %>
  <h4>タイトル</h4>
  <%= f.text_field :title %>
  <h4>本文</h4>
  <%= f.text_area :introduction %>
  <%= f.submit '投稿' %>
<%end%>
<%= link_to "削除", content_path(@content), method: :delete %>