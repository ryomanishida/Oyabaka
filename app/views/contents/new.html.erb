<%= render 'layouts/navber' %>
<%= render 'layouts/content_error' %>
<div class="container my-5">
  <%= form_with model:@content, local:true do |f| %>
    <%= f.label :img, "File", class: "image_input_btn" %><br>
    <%= f.file_field :img, class: "image image_form" %>
    <%= f.label :title %>
    <%= f.text_field :title %>
    <%= f.label :introduction %>
    <%= f.text_area :introduction %>

    <h6><%= t ".ex) dog,food,cute" %></h6>
    <div class="field">
      <%= f.label :tag %>
      <%= f.text_field :tag_names, placeholder: t("contents.new.please separate with,") %>
    </div>
    <%= f.submit t("dictionary.button.create") %>
  <%end%>
</div>
